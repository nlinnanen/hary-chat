    },
    "type": {
      "type": "string",
      "regex": "(multiselect|singleselect|range|input)",
      "required": true,
      "default": "singleselect"
    },
    "isFirst": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
